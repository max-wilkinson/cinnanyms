{
  "name": "wordsmith",
  "displayName": "Wordsmith",
  "description": "Suggests synonyms for the selected word. Useful when trying to find the perfect name for a variable or class.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.wordsmith"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.wordsmith",
        "title": "Wordsmith"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "@types/request": "^2.48.3",
    "@types/request-promise-native": "^1.0.17",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  }
}
