{
  "name": "cinnanyms",
  "displayName": "Cinnanyms",
  "description": "Suggests synonyms when trying to find the perfect name for a variable or class.",
  "license": "SEE LICENSE IN LICENSE FILE",
  "keywords": [
    "word",
    "name",
    "synonym"
  ],
  "icon": "CinnanymsIcon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/max-wilkinson/cinnanyms.git"
  },
  "publisher": "FordLabs",
  "preview": true,
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.cinnanyms"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.cinnanyms",
        "title": "Cinnanyms"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "@types/request": "^2.48.3",
    "@types/request-promise-native": "^1.0.17",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  }
}
